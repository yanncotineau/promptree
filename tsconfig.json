{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "jsx": "react",
    "jsxFactory": "Promptree.createElement",
    "jsxFragmentFactory": "Fragment",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": ["src/**/*"]
}